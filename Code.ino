void setup() {

  Wire.begin(D2, D1);  // SDA, SCL

  //  Start OLED
  if (!display.begin(SSD1306_SWITCHCAPVCC, OLED_ADDR)) {
    while (true); // 
  }


  display.clearDisplay();

}


void loop() {

  for(int i = 0; i < epd_bitmap_allArray_LEN; i++) {

    display.clearDisplay();

    display.drawBitmap(
      0, 0,
      epd_bitmap_allArray[i],
      128, 64,
      1
    );

    display.display();

    delay(150); //speed of animation
  }
}
